# API URL
VITE_API_URL=http://localhost:3000

# App
VITE_APP_NAME=ePlatform
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_AUTH=true
VITE_ENABLE_REGISTRATION=true

# Development
VITE_DEV_LOGIN_EMAIL=admin@eplatform.local
VITE_DEV_LOGIN_PASSWORD=admin123

# Analytics (disabled in development)
VITE_ANALYTICS_ID=

# Error Reporting (disabled in development)
VITE_SENTRY_DSN=

# Environment
VITE_NODE_ENV=development

# Localization
VITE_DEFAULT_LANGUAGE=ro
VITE_AVAILABLE_LANGUAGES=ro,en

# Cache
VITE_CACHE_TTL=3600

# API Timeouts
VITE_API_TIMEOUT=30000
VITE_API_LONG_TIMEOUT=60000 